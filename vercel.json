{
  "version": 2,
  "builds": [
    {
      "src": "proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/proxy",
      "dest": "/proxy.js"
    },
    {
      "src": "/Cadastro.html",
      "dest": "/Cadastro.html"
    },
    {
      "src": "/leitor.html",
      "dest": "/leitor.html"
    },
    {
      "src": "/LeitorScript.js",
      "dest": "/LeitorScript.js"
    },
    {
      "src": "/CadastroScript.js",
      "dest": "/CadastroScript.js"
    },
    {
      "src": "/CadastroStyle.css",
      "dest": "/CadastroStyle.css"
    },
    {
      "src": "/leitorStyle.css",
      "dest": "/leitorStyle.css"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
